<Application x:Class="NxEditor.TotkPlugin.Demo.App"
             xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:NxEditor.TotkPlugin.Demo"
             RequestedThemeVariant="Default">

    <Application.DataTemplates>
        <local:ViewLocator />
    </Application.DataTemplates>

    <Application.Resources>
        <FontFamily x:Key="UbuntuMono">/Assets/Fonts/UbuntuMono/#Ubuntu Mono</FontFamily>
        <FontFamily x:Key="Ubuntu">/Assets/Fonts/Ubuntu/#Ubuntu</FontFamily>
    </Application.Resources>

    <Application.Styles>
        <FluentTheme />
        <StyleInclude Source="avares://AvaloniaEdit/Themes/Fluent/AvaloniaEdit.xaml" />
    </Application.Styles>
</Application>